{
  "name": "@internal/cache-response",
  "version": "1.0.0",
  "description": "RFC 7234 compliant HTTP response caching with Cache-Control, conditional requests, and stale-while-revalidate support",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "default": "./dist/types.mjs"
    },
    "./parser": {
      "import": "./dist/parser.mjs",
      "default": "./dist/parser.mjs"
    },
    "./cache": {
      "import": "./dist/cache.mjs",
      "default": "./dist/cache.mjs"
    },
    "./stores": {
      "import": "./dist/stores/index.mjs",
      "default": "./dist/stores/index.mjs"
    },
    "./stores/memory": {
      "import": "./dist/stores/memory.mjs",
      "default": "./dist/stores/memory.mjs"
    }
  },
  "files": [
    "dist/**/*.mjs"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --coverage"
  },
  "keywords": [
    "mta",
    "cache-response",
    "http-cache",
    "rfc7234",
    "cache-control",
    "etag",
    "conditional-request",
    "stale-while-revalidate",
    "esm",
    "mjs"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "peerDependencies": {
    "ioredis": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "ioredis": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "typescript": "^5.7.3"
  }
}
