{
  "name": "@internal/fetch-client",
  "version": "1.0.0",
  "description": "Enterprise-grade, latency-optimized HTTP client with streaming support",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./types": {
      "import": "./dist/types.mjs",
      "default": "./dist/types.mjs"
    },
    "./config": {
      "import": "./dist/config.mjs",
      "default": "./dist/config.mjs"
    },
    "./factory": {
      "import": "./dist/factory.mjs",
      "default": "./dist/factory.mjs"
    },
    "./fastify": {
      "import": "./dist/integrations/fastify-plugin.mjs",
      "default": "./dist/integrations/fastify-plugin.mjs"
    },
    "./streaming/sse": {
      "import": "./dist/streaming/sse-reader.mjs",
      "default": "./dist/streaming/sse-reader.mjs"
    },
    "./streaming/ndjson": {
      "import": "./dist/streaming/ndjson-reader.mjs",
      "default": "./dist/streaming/ndjson-reader.mjs"
    }
  },
  "files": [
    "dist/**/*.mjs"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "test": "NODE_OPTIONS='--experimental-vm-modules' jest --passWithNoTests",
    "test:coverage": "NODE_OPTIONS='--experimental-vm-modules' jest --passWithNoTests --coverage"
  },
  "keywords": [
    "mta",
    "fetch",
    "client",
    "http",
    "sse",
    "streaming",
    "llm",
    "undici",
    "esm",
    "mjs"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "pino": "^9.0.0",
    "pino-pretty": "^13.0.0",
    "@internal/fetch-auth-encoding": "workspace:*"
  },
  "peerDependencies": {
    "@internal/fetch-proxy-dispatcher": "workspace:*",
    "@internal/app-static-config-yaml": "workspace:*",
    "undici": "^7.0.0"
  },
  "peerDependenciesMeta": {
    "@internal/app-static-config-yaml": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "fastify-plugin": {
      "optional": true
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-typescript": "^7.24.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.10.0",
    "babel-jest": "^29.7.0",
    "fastify": "^5.0.0",
    "fastify-plugin": "^5.0.1",
    "jest": "^29.7.0",
    "typescript": "^5.7.3"
  }
}